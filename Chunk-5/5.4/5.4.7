public class DragonTester
{
    public static void main(String[] args)
    {
        Dragon nofic = new Dragon("nofic", 90);
        System.out.println(nofic.getLevel());
        System.out.println(nofic.getName());
        System.out.println(nofic.isFireBreather());
        nofic.attack();
    }
}
---------------------------------------------------------------
public class Dragon 
{
    private String name;
    private int level;
    private boolean canBreatheFire;
    
    // Write the constructor here!
    public Dragon(String n, int l){
        name = n;
        level = l;
        if(level >= 70){
            canBreatheFire = true;
        }else{
            canBreatheFire = false;
        }
    }
    // Put getters here
    public String getName(){
        return name;
    }
    public int getLevel(){
        return level;
    }
    public boolean isFireBreather(){
        return canBreatheFire;
    }
    // Put other methods here
    public void attack(){
        if(canBreatheFire == true){
            System.out.println(">>>>>>>>>>\n>>>>>>>>>>>>>>\n>>>>>>>>>>>>>>\n>>>>>>>>>>");
        }else{
            System.out.println("~ ~ ~");
        }
    }
    public void gainExperience(){
        level = level + 10;
        if(level >= 70){
            canBreatheFire = true;
        }else{
            canBreatheFire = false;
        }
    }

    // String representation of the object
    public String toString()
    {
        return "Dragon " + name + " is at level " + level;
    }
}
